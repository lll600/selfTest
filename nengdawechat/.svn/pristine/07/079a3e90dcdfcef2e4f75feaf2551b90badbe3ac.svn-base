
.search-main-contain .search-contain-bg {
  background: #fff;
  height: 40rpx;
  width: 100%;
  position: fixed;
  z-index: 9;
}
.search-main-contain .search-contain {
  background: #fff;
  height: 80rpx;
  width: 100%;
  line-height: 80rpx;
  color: #666;
  font-size: 32rpx;
  position: fixed;
  top: 40rpx;
  display: flex;
  z-index: 9;
}
.search-main-contain .search-contain .icon-arrow_left {
  line-height: 80rpx;
  margin-left: 5%;
}
.search-main-contain .search-contain .has-back {
  width: 65%;
  margin-left: 2%;
}
.search-main-contain .search-contain .no-back {
  margin-left: 5%;
  width: 70%;
}
.search-main-contain .search-contain .search-left {
  background: #f8f8f8;
  height: 60rpx;
  float: left;
  margin-top: 10rpx;
  border-radius: 30rpx;
  line-height: 60rpx;
  display: flex;
  align-items: center;
}
.search-main-contain .search-contain .search-left .iconfont {
  font-size: 32rpx;
  padding-left: 20rpx;
}
.search-main-contain .search-contain .search-left ._input {
  height: 60rpx;
  width: 50%;
  position: absolute;
  margin-left: 10%;
}
.search-main-contain .search-contain .search-left .icon-clear {
  position: absolute;
  right: 27.5%;
  color: #bfbfbf;
}
.search-main-contain .search-contain .search-right {
  width: 15%;
  text-align: center;
  float: left;
}
.search-main-contain .search-contain .search-right .iconfont {
  font-size: 40rpx;
}
.homeContain .place {
  height: 110rpx;
}
.homeContain .homeMenu {
  position: fixed;
  bottom: 0;
  height: 110rpx;
  width: 100%;
  background: #fff;
  border-top: 2rpx solid #cfcfcf;
  box-shadow: 0 0  10rpx 0 #cfcfcf;
}
.homeContain .homeMenu .menuTitle {
  width: 20%;
  float: left;
  height: 100%;
  color: #cfcfcf;
  text-align: center;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.homeContain .homeMenu .menuTitle ._i {
  font-size: 40rpx;
}
.homeContain .homeMenu .menuActive {
  color: #ee781b;
}