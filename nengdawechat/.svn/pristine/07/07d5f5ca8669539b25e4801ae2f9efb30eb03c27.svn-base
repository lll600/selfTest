
.search-main-contain .capsule-holder {
  background: red;
  width: 200rpx;
  height: 100%;
}
.search-main-contain .search-contain-bg {
  background: #fff;
  width: 100%;
  position: fixed;
  z-index: 9;
}
.search-main-contain .search-contain {
  background: #fff;
  width: 100%;
  color: #666;
  font-size: 32rpx;
  position: fixed;
  display: flex;
  align-items: center;
  z-index: 9;
}
.search-main-contain .search-contain .search-back {
  padding-left: 5%;
}
.search-main-contain .search-contain .search-back .icon-arrow_left {
  line-height: 80rpx;
}
.search-main-contain .search-contain .has-back {
  width: 55%;
  margin-left: 2%;
}
.search-main-contain .search-contain .no-back {
  margin-left: 4%;
  width: 60%;
}
.search-main-contain .search-contain .search-left {
  border: 2rpx solid #eaeaea;
  height: 64rpx;
  float: left;
  border-radius: 30rpx;
  line-height: 64rpx;
  display: flex;
  align-items: center;
}
.search-main-contain .search-contain .search-left .iconfont {
  font-size: 32rpx;
  padding-left: 20rpx;
}
.search-main-contain .search-contain .search-left ._input {
  height: 60rpx;
  width: 50%;
  position: absolute;
  margin-left: 10%;
}
.search-main-contain .search-contain .search-left .icon-clear {
  position: absolute;
  right: 27.5%;
  color: #bfbfbf;
}
.search-main-contain .search-contain .search-right {
  width: 15%;
  text-align: center;
  float: left;
}
.search-main-contain .search-contain .search-right .iconfont {
  font-size: 40rpx;
}
.shodow {
  width: 100%;
  height: 100vh;
  background: black;
  opacity: 0.5;
  position: fixed;
  z-index: 1000;
  top: 0;
}
.modal {
  width: 60%;
  height: 36vh;
  position: fixed;
  z-index: 99999;
  background: white;
  top: 50%;
  left: 50%;
  margin-left: -30%;
  margin-top: -18vh;
  font-size: 28rpx;
  border-radius: 20rpx;
}
.modal ._div {
  text-align: center;
  margin: 40rpx;
}
.modal ._img {
  width: 200rpx;
  height: 200rpx;
}
.modal .colorRed {
  color: red;
  margin: 0;
}
.modal .img {
  margin-bottom: 0;
}
.modal .cancelDiv {
  border-top: 2rpx solid #bababa;
  color: #fa9813;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0;
  position: absolute;
  bottom: 0;
  width: 100%;
  font-size: 32rpx;
}
.homeContain .place {
  height: 110rpx;
}
.homeContain .homeMenu {
  position: fixed;
  bottom: 0;
  height: 110rpx;
  width: 100%;
  background: #fff;
  border-top: 2rpx solid #cfcfcf;
  box-shadow: 0 0  10rpx 0 #cfcfcf;
}
.homeContain .homeMenu .menuTitle {
  width: 20%;
  float: left;
  height: 100%;
  color: #cfcfcf;
  text-align: center;
  font-size: 28rpx;
  margin-top: 20rpx;
}
.homeContain .homeMenu .menuTitle ._i {
  font-size: 40rpx;
}
.homeContain .homeMenu .menuActive {
  color: #ee781b;
}