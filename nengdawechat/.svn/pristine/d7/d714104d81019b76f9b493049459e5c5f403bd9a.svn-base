}
.singleAnswer ._article .mint-cell-title {
  width: 100%;
}
.singleAnswer ._article .saveBottom {
  width: 100%;
  position: fixed;
  bottom: 0;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
  z-index: 100;
  border-top: 4rpx solid #eaeaea;
  font-size: 34rpx;
}
.singleAnswer ._article .showShare {
  width: 100%;
  height: 200rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.singleAnswer ._article .showShare ._img {
  width: 50%;
}
.singleAnswer ._article .showShare ._div {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
}
.singleAnswer ._article .showSave {
  width: 100%;
  height: 600rpx;
}
.singleAnswer ._article .showSave .saveTop {
  width: 100%;
  height: 100rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.singleAnswer ._article .showSave .saveTop ._p:nth-of-type(1) {
  color: #fa9813;
  font-size: 32rpx;
}
.singleAnswer ._article .showSave .saveTop ._p:nth-of-type(2) {
  color: #444444;
  font-size: 34rpx;
}
.singleAnswer ._article .showSave .saveTop ._p:nth-of-type(3) {
  color: #000;
  font-size: 26rpx;
}
.singleAnswer ._article .overHiden {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
