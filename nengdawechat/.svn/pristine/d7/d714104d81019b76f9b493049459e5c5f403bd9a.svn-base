}
.singleAnswer ._article .introduce {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
  width: 100%;
  height: 90rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #eaeaea;
}
.singleAnswer ._article .introduce .introduceLeft {
  display: inline-flex;
  align-items: center;
  width: 70%;
}
.singleAnswer ._article .introduce .introduceLeft ._span {
  font-size: 24rpx;
  color: #99999a;
}
.singleAnswer ._article .introduce ._img {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.singleAnswer ._article .attention {
  height: 60rpx;
  border-radius: 10rpx;
  display: inline-flex;
  justify-content: center;
  align-items: center;
  font-size: 28.000000000000004rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
}
.singleAnswer ._footer {
  width: 100%;
  height: 90rpx;
  background: white;
  position: fixed;
  bottom: 0;
  z-index: 100;
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  border-top: 2rpx solid #eaeaea;
}
.singleAnswer ._footer ._p {
  border-right: 6rpx solid #eaeaea;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 32%;
}
.singleAnswer ._footer .noBorder {
  border-right: none;
}
.singleAnswer ._footer ._span {
  padding-left: 20rpx;
}
.singleAnswer ._footer .orange:before {
  color: #fe9500;
}
.userDeatil {
  width: 85%;
}
.userDeatil ._h5 {
  font-size: 32rpx;
}