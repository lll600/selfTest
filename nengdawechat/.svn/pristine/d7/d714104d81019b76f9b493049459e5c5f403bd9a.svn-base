}
.singleAnswer ._article .answer ._img {
  max-width: 100%;
  max-height: 600rpx;
}
.singleAnswer ._article .title {
  padding: 30rpx;
  border-bottom: 10rpx solid #eaeaea;
}
.singleAnswer ._article .title ._h4 {
  font-size: 32rpx;
  word-wrap: break-word;
}
.singleAnswer ._article .title .answerC {
  color: #99999a;
  font-size: 28.000000000000004rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 20rpx;
}
.singleAnswer ._article .title .answerC .writeAnswer {
  border: 2rpx solid #99999a;
  border-radius: 8rpx;
  height: 57.99999999999999rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border: 2rpx solid #646464;
  padding: 0 10rpx;
}
.singleAnswer ._article .title .icon-arrow {
  margin-left: 10rpx;
  font-size: 24rpx;
  display: inline;
}
.singleAnswer ._article .icon-gzwt {
