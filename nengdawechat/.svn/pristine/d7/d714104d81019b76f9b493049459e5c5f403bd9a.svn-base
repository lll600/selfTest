  word-wrap: break-word;
}
.singleAnswer ._article .answer ._div {
  font-size: 32rpx;
  max-width: 100%;
}
.singleAnswer ._article .answer ._h1,
.singleAnswer ._article .answer ._h2,
.singleAnswer ._article .answer ._h3,
.singleAnswer ._article .answer ._h4,
.singleAnswer ._article .answer ._h5,
.singleAnswer ._article .answer ._img,
.singleAnswer ._article .answer ._p {
